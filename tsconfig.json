{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es2019",
    "lib": ["ESNext"],
    "declaration": true,
    "sourceMap": true,
    "removeComments": false,
    "alwaysStrict": true,
    "pretty": true,
    "outDir": "dist",
    "incremental": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": true,
  }
}
